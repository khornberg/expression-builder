{
  "name": "expression-builder",
  "version": "1.6.0",
  "description": "Build expressions with a GUI",
  "keywords": [
    "ember-addon",
    "expression",
    "formula",
    "formulae",
    "builder"
  ],
  "license": "MIT",
  "author": "Kyle Hornberg",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/khornberg/expression-builder",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember try:each",
    "docs": "ember build -p && rm -r docs && rsync -av --progress dist/ docs --exclude tests"
  },
  "dependencies": {
    "ember-cli-babel": "7.1.2",
    "ember-cli-htmlbars": "3.0.1"
  },
  "devDependencies": {
    "broccoli-asset-rev": "3.0.0",
    "ember-ajax": "4.0.0",
    "ember-cli": "2.18.2",
    "ember-cli-dependency-checker": "3.0.0",
    "ember-cli-eslint": "4.2.3",
    "ember-cli-htmlbars-inline-precompile": "1.0.5",
    "ember-cli-inject-live-reload": "2.0.1",
    "ember-cli-qunit": "4.4.0",
    "ember-cli-shims": "1.2.0",
    "ember-cli-sri": "2.1.1",
    "ember-cli-uglify": "2.1.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.0",
    "ember-load-initializers": "1.1.0",
    "ember-qunit-nice-errors": "1.2.0",
    "ember-resolver": "5.0.1",
    "ember-source": "2.18.2",
    "loader.js": "4.7.0"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
